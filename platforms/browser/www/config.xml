<?xml version='1.0' encoding='utf-8'?>
<widget id="com.galeriainmobiliaria.arriendosenbucaramanga" version="1.0.52" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>GaleriaInmobiliaria-ArriendosEnBucaramanga</name>
    <description>
        Aplicación para galería inmobiliaria
    </description>
    <author email="info@aedigital.co" href="http://aedigital.co">
        AE Digital
    </author>
    <content src="index.html" />
    <icon src="www/res/icon/ios/icon-1024.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="17" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="ShowSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
